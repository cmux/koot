{
    "name": "koot-boilerplate",
    "private": true,
    "version": "0.15.0-rc.2",
    "description": "Boilerplate project for koot.js app",
    "author": {
        "name": "diablohu",
        "email": "diablohudream@gmail.com",
        "url": "http://diablohu.com"
    },
    "homepage": "https://github.com/cmux/koot#readme",
    "license": "none",
    "repository": {
        "type": "git",
        "url": "https://github.com/cmux/koot.git"
    },
    "engines": {
        "node": ">=12.20.0"
    },
    "scripts": {
        "start": "koot-start",
        "build": "koot-build",
        "dev": "koot-dev",
        "analyze": "koot-analyze",
        "eslint": "eslint \"src/**/*.+(js|jsx)\"",
        "check-types": "tsc --noEmit",
        "prepare": "husky install",
        "up": "yarn upgrade-interactive --latest"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "bind-event": "^1.1.2",
        "classnames": "^2.3.1",
        "koot": "^0.15.0-rc.2"
    },
    "devDependencies": {
        "@types/classnames": "^2.3.0",
        "autoprefixer": "^10.3.2",
        "babel-eslint": "^10.1.0",
        "cssnano": "^5.0.8",
        "eslint": "^7.32.0",
        "eslint-config-koot": "^3.1.1",
        "husky": "^7.0.2",
        "lint-staged": "^11.1.2",
        "normalize.css": "^8.0.1",
        "prettier": "^2.3.2",
        "prettier-eslint": "^13.0.0",
        "redux-logger": "^3.0.6",
        "svg-url-loader": "^7.1.1"
    },
    "bugs": {
        "url": "https://github.com/cmux/koot/issues"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,cjs,mjs,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,md,css,less,sass,scss}": [
            "prettier --write"
        ]
    }
}
