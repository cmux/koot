{
    "name": "koot-boilerplate-legacy",
    "private": true,
    "version": "0.14.8",
    "description": "Koot.js 模板项目",
    "author": {
        "name": "Diablohu",
        "email": "diablohudream@gmail.com",
        "url": "http://diablohu.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/cmux/koot-boilerplate"
    },
    "license": "Apache-2.0",
    "scripts": {
        "start": "koot-start",
        "start:server": "koot-start --no-build",
        "build": "koot-build",
        "build:isomorphic:client": "koot-build -c",
        "build:isomorphic:server": "koot-build -s",
        "build:spa": "koot-build --type react-spa --dest dist-spa",
        "build:spa:use-config-file": "koot-build --config koot.config.spa.js",
        "dev": "koot-dev",
        "dev:no-open": "koot-dev --no-open",
        "dev:global-pm2": "koot-dev -g",
        "dev:isomorphic:client": "koot-dev -c",
        "dev:isomorphic:server": "koot-dev -s",
        "dev:spa": "koot-dev --type react-spa --dest dist-spa",
        "analyze": "koot-analyze",
        "eslint": "eslint \"src/**/*.+(js|jsx)\""
    },
    "dependencies": {
        "axios": "^0.19.0",
        "koot": "^0.14.8"
    },
    "devDependencies": {
        "cssnano": "^4.1.10",
        "normalize.css": "^8.0.1",
        "svg-url-loader": "^2.3.2"
    },
    "engines": {
        "node": ">=10.0.0"
    }
}
