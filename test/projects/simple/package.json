    "scripts": {
        "start": "koot-start",
        "start:server": "koot-start --no-build",
        "start:spa": "koot-start --type react-spa",
        "start:electron": "koot-start --config koot.config.electron.js",
        "build": "koot-build",
        "build:spa": "koot-build --type react-spa",
        "build:client": "koot-build -c",
        "build:server": "koot-build -s",
        "build:electron": "koot-build --config koot.config.electron.js",
        "dev": "koot-dev",
        "dev:client": "koot-dev -c",
        "dev:server": "koot-dev -s",
        "dev:global-pm2": "koot-dev -g",
        "dev:no-open": "koot-dev --no-open",
        "dev:custom-port": "koot-dev --no-open --port 8081",
        "dev:electron": "koot-dev --config koot.config.electron.js",
        "analyze": "koot-analyze",
        "analyze:spa": "koot-analyze --type react-spa",
        "eslint": "eslint \"src/**/*.+(js|jsx)\"",
        "koot-buildtest-isomorphic-build": "koot-build --env prod --koot-test",
        "koot-buildtest-isomorphic-start-server": "koot-start --no-build",
        "koot-buildtest-isomorphic-start-server-custom-port": "koot-start --no-build --port 8080",
        "koot-buildtest-isomorphic-dev": "koot-dev --no-open",
        "koot-basetest-prod": "koot-start --koot-test -- bbbbb=a1b2c3",
        "koot-basetest-isomorphic-dev": "koot-dev --no-open --koot-test -- bbbbb=a1b2c3",
        "koot-basetest-no_bundle_versions_keep": "koot-build -c --config koot.config.no-bundles-keep.js",
        "koot-basetest-bundle_versions_keep": "koot-build -c --config koot.config.bundles-keep.js"
    },
