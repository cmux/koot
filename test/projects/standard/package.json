    "scripts": {
        "start": "koot-start",
        "start:0.8:full": "koot-start --config koot.config.0.8-full.js",
        "start:server": "koot-start --no-build",
        "start:spa": "koot-start --type spa --dest dist-spa",
        "start:serverless": "koot-start --config koot.config.serverless.js",
        "start:public-path": "koot-start --config koot.config.public-path.js",
        "start:normal": "koot-build --config koot.config.public-path.js && node dist-public-path/server/index.js",
        "start:inspect": "koot-build --config koot.config.test-ssr-memory-leak.js && node --inspect dist-test-ssr-memory-leak/server/index.js",
        "build": "koot-build",
        "build:client": "koot-build -c",
        "build:server": "koot-build -s",
        "build:spa": "koot-build --type spa --dest dist-spa",
        "build:spa:use-config-file": "koot-build --config koot.config.spa.js",
        "build:public-path": "koot-build --config koot.config.public-path.js",
        "build:serverless": "koot-build --config koot.config.serverless.js",
        "dev": "koot-dev",
        "dev:0.8:full": "koot-dev --config koot.config.0.8-full.js",
        "dev:client": "koot-dev -c",
        "dev:client:dev": "koot-dev -c --koot-development",
        "dev:server": "koot-dev -s",
        "dev:dev": "koot-dev --no-open --koot-development",
        "dev:global-pm2": "koot-dev -g",
        "dev:no-open": "koot-dev --no-open",
        "dev:spa": "koot-dev --type spa --dest dist-spa",
        "dev:spa:no-open": "koot-dev --type spa --dest dist-spa --no-open",
        "analyze": "koot-analyze",
        "analyze:spa": "koot-analyze --type spa",
        "eslint": "eslint \"src/**/*.+(js|jsx)\"",
        "test:server": "npm run build:server && node dist\\server\\index.js",
        "koot-buildtest-isomorphic-build": "koot-build --env prod --koot-test",
        "koot-buildtest-isomorphic-start-server": "koot-start --no-build --koot-test",
        "koot-buildtest-isomorphic-start-server-custom-port": "koot-start --no-build --port 8316 --koot-test",
        "koot-buildtest-isomorphic-standard-development": "koot-dev --no-open --koot-test --config koot.config.js",
        "koot-buildtest-isomorphic-i18n_use_router-production": "koot-start --koot-test --config koot.config.i18n-use-router.js",
        "koot-buildtest-isomorphic-i18n_use_router-development": "koot-dev --no-open --koot-test --config koot.config.i18n-use-router.js",
        "koot-buildtest-isomorphic-no_bundles_keep-production": "koot-start --koot-test --config koot.config.no-bundles-keep.js",
        "koot-buildtest-isomorphic-no_bundles_keep-development": "koot-dev --no-open --koot-test --config koot.config.no-bundles-keep.js",
        "koot-buildtest-isomorphic-config_old_0.6-production": "koot-start --koot-test --config koot.config.old-0.6.js",
        "koot-buildtest-isomorphic-config_old_0.6-development": "koot-dev --no-open --koot-test --config koot.config.old-0.6.js",
        "koot-buildtest-spa-build-config-default": "koot-build --koot-test --type react-spa --dest dist-spa-test",
        "koot-buildtest-spa-build-config-koot_build_spa-1_js": "koot-build --koot-test --config koot.build.spa-1.js",
        "koot-buildtest-spa-build-config-koot_build_spa-2_js": "koot-build --koot-test --config koot.build.spa-2.js",
        "koot-buildtest-spa-build-config-koot_build_spa-3_js": "koot-build --koot-test --config koot.build.spa-3.js",
        "koot-buildtest-isomorphic-public_path-production": "koot-start --koot-test --config koot.config.public-path.js",
        "koot-buildtest-isomorphic-public_path-development": "koot-dev --no-open --koot-test --config koot.config.public-path.js",
        "koot-buildtest-spa-build-config-koot_build_spa-#249_js": "koot-build --koot-test --config koot.build.spa-#249.js"
    },
